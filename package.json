{
  "homepage": "http://totato4.github.io/artgal",
  "name": "artgal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0",
    "lint:perf": "set TIMING=1 && eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "pretty-quick --ignore-path .gitignore",
    "prettier:all": "prettier --write . --ignore-path .gitignore",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.14",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.66.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
